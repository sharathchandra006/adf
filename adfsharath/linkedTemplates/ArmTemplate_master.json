{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfsharath"
		},
		"ds_csv_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ds_csv_ls'"
		},
		"outcsv_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'outcsv'"
		},
		"ds_csv_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://shaadf.dfs.core.windows.net/"
		},
		"outcsv_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://shaadf.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfsharath_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ds_csv_ls_accountKey": {
						"value": "[parameters('ds_csv_ls_accountKey')]"
					},
					"outcsv_accountKey": {
						"value": "[parameters('outcsv_accountKey')]"
					},
					"ds_csv_ls_properties_typeProperties_url": {
						"value": "[parameters('ds_csv_ls_properties_typeProperties_url')]"
					},
					"outcsv_properties_typeProperties_url": {
						"value": "[parameters('outcsv_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}